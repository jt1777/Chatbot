# Production Environment Configuration
# This file contains production-specific environment variables

# Production API Configuration - Replace with your actual production API URL
EXPO_PUBLIC_API_BASE_URL=https://your-production-api.com

# Production Environment
EXPO_PUBLIC_ENV=production

# App Configuration
EXPO_PUBLIC_APP_NAME=Ask Akasha
EXPO_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
EXPO_PUBLIC_ENABLE_DEBUG=false
EXPO_PUBLIC_ENABLE_ANALYTICS=true

# Optional: Custom production settings
EXPO_PUBLIC_LOG_LEVEL=error
